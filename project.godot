[application]

config/name="Insomnia"
run/main_scene="res://src/rooms/menu/r_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Insomnia"
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

Global="*res://src/scripts/global.gd"
RGlobal="*res://src/rooms/global/r_global.tscn"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=960
window/size/test_height=540
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[logging]

file_logging/log_path="user://traceback.log"
file_logging/max_log_files=1

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/intended_usage/framebuffer_allocation.mobile=1
environment/default_environment="res://default_env.tres"
